{
  "name": "questarr",
  "version": "1.3.0",
  "description": "A video game management application inspired by the -Arr apps. Track and organize your video game collection with automated discovery and download management.",
  "type": "module",
  "packageManager": "npm@11.7.0",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/Doezer/Questarr.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && tsc -p tsconfig.build.json",
    "start": "NODE_ENV=production node dist/server/index.js",
    "check": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css}\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx server/run-migrations.ts",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "dev:test": "tsx scripts/clean-test-db.ts && cross-env SQLITE_DB_PATH=data/test.db PORT=5100 npm run dev",
    "prepare": "husky"
  },
